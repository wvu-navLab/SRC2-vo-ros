<launch>

  <!-- launch image selector -->
  <include file="$(find image_selection)/launch/selectScout.launch" />

  <!-- launch vo -->
  <group ns="scout_1">
    <node pkg="wvu_vo_ros"
          name="ros_stereo_vo_node"
          type="ros_stereo_vo_node"
          output="screen">
    </node>

    <rosparam command="load" file="$(find wvu_vo_ros)/config/scout_parameters.yaml"/>

    <param name ="/use_sim_time" value="true"/>

  </group>

</launch>
